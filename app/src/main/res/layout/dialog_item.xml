<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:app="http://schemas.android.com/apk/res-auto"
	xmlns:tools="http://schemas.android.com/tools"
	android:layout_width="wrap_content"
	android:layout_height="wrap_content"
	android:paddingEnd="48.0px"
	tools:visibility="visible">

	<ImageView
		android:id="@+id/item_bg"
		android:layout_width="0.0dip"
		android:layout_height="0.0dip"
		android:scaleType="fitXY"
		app:layout_constraintBottom_toBottomOf="parent"
		app:layout_constraintEnd_toEndOf="parent"
		app:layout_constraintStart_toStartOf="parent"
		app:layout_constraintTop_toTopOf="parent"
		app:srcCompat="@drawable/ic_dialog_select" />

	<TextView
		android:id="@+id/item_field1"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:layout_marginStart="@dimen/_4sdp"
		android:fontFamily="@font/roboto_regular"
		android:lines="1"
		android:maxLines="1"
		android:paddingTop="@dimen/_3sdp"
		android:paddingEnd="@dimen/_5sdp"
		android:paddingBottom="@dimen/_3sdp"
		android:singleLine="true"
		android:text="Field1\u0009"
		android:textColor="#ffffffff"
		android:textSize="@dimen/_8sdp"
		app:layout_constraintStart_toStartOf="parent"
		app:layout_constraintTop_toTopOf="parent" />

	<TextView
		android:id="@+id/item_field2"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:fontFamily="@font/roboto_regular"
		android:lines="1"
		android:maxLines="1"
		android:paddingTop="@dimen/_3sdp"
		android:paddingEnd="@dimen/_5sdp"
		android:paddingBottom="@dimen/_3sdp"
		android:singleLine="true"
		android:text="Field2\u0009"
		android:textColor="#ffffffff"
		android:textSize="@dimen/_8sdp"
		app:layout_constraintStart_toEndOf="@+id/item_field1"
		app:layout_constraintTop_toTopOf="parent" />

	<TextView
		android:id="@+id/item_field3"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:fontFamily="@font/roboto_regular"
		android:lines="1"
		android:maxLines="1"
		android:paddingTop="@dimen/_3sdp"
		android:paddingEnd="@dimen/_5sdp"
		android:paddingBottom="@dimen/_3sdp"
		android:singleLine="true"
		android:text="Field3\u0009"
		android:textColor="#ffffffff"
		android:textSize="@dimen/_8sdp"
		app:layout_constraintStart_toEndOf="@+id/item_field2"
		app:layout_constraintTop_toTopOf="parent" />

	<TextView
		android:id="@+id/item_field4"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:fontFamily="@font/roboto_regular"
		android:lines="1"
		android:maxLines="1"
		android:paddingTop="@dimen/_3sdp"
		android:paddingEnd="@dimen/_5sdp"
		android:paddingBottom="@dimen/_3sdp"
		android:singleLine="true"
		android:text="Field4\u0009"
		android:textColor="#ffffffff"
		android:textSize="@dimen/_8sdp"
		app:layout_constraintStart_toEndOf="@+id/item_field3"
		app:layout_constraintTop_toTopOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>